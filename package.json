    "awesome-typescript-loader": "^5.2.0",
    "css-loader": "^1.0.0",
    "emotion": "^9.2.8",
    "file-loader": "^2.0.0",
    "font-awesome": "^4.7.0",
    "history": "^4.6.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.0",
    "lodash": "^4.17.10",
    "prettier": "^1.14.2",
    "randomcolor": "^0.5.3",
    "react-dom": "^16.4.2",
    "ruled-router": "^0.2.2",
    "style-loader": "^0.23.0",
